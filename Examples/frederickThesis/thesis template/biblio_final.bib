@inproceedings{Ulges2008,
	Abstract = {A key problem with the automatic detection of semantic concepts (like$\backslash$n'interview' or 'soccer') in video streams is the manual acquisition$\backslash$nof adequate training sets. Recently, we have proposed to use online$\backslash$nvideos downloaded from portals like youtube.com for this purpose,$\backslash$nwhereas tags provided by users during video upload serve as ground$\backslash$ntruth annotations.$\backslash$n$\backslash$n$\backslash$nThe problem with such training data is that it is weakly labeled:$\backslash$nAnnotations are only provided on video level, and many shots of a$\backslash$nvideo may be "non-relevant", i.e. not visually related to a tag.$\backslash$nIn this paper, we present a probabilistic framework for learning$\backslash$nfrom such weakly annotated training videos in the presence of irrelevant$\backslash$ncontent. Thereby, the relevance of keyframes is modeled as a latent$\backslash$nrandom variable that is estimated during training.$\backslash$n$\backslash$n$\backslash$nIn quantitative experiments on real-world online videos and TV news$\backslash$ndata, we demonstrate that the proposed model leads to a significantly$\backslash$nincreased robustness with respect to irrelevant content, and to a$\backslash$nbetter generalization of the resulting concept detectors.},
	Author = {Ulges, Adrian and Schulze, Christian and Keysers, Daniel and Breuel, Thomas},
	Booktitle = {Proceedings of the Conference on Image and Video Retrieval (CIVR)},
	Doi = {10.1145/1386352.1386358},
	Isbn = {978-1-60558-070-8},
	Pages = {9--16},
	Title = {{Identifying relevant frames in weakly labeled videos for training concept detectors}},
